package com.baidu.gameqa.Gat.dataobject.testcase;


public class TestStep 
{
	private String StepAssembly;
	private String StepGroup;
	private String StepName;
	private String StepParameterID;
	private String StepParametersFilePath;
	
	/**
	 * @return the stepAssembly
	 */
	public String getStepAssembly() {
		return StepAssembly;
	}
	
	
	/**
	 * @param stepAssembly the stepAssembly to set
	 */
	public void setStepAssembly(String stepAssembly) {
		StepAssembly = stepAssembly;
	}
	
	/**
	 * @return the stepGroup
	 */
	public String getStepGroup() {
		return StepGroup;
	}
	
	
	/**
	 * @param stepGroup the stepGroup to set
	 */
	public void setStepGroup(String stepGroup) {
		StepGroup = stepGroup;
	}


	/**
	 * @return the stepName
	 */
	public String getStepName() {
		return StepName;
	}


	/**
	 * @param stepName the stepName to set
	 */
	public void setStepName(String stepName) {
		StepName = stepName;
	}


	/**
	 * @return the stepParameterID
	 */
	public String getStepParameterID() {
		return StepParameterID;
	}


	/**
	 * @param stepParameterID the stepParameterID to set
	 */
	public void setStepParameterID(String stepParameterID) {
		StepParameterID = stepParameterID;
	}


	/**
	 * @return the stepParametersFilePath
	 */
	public String getStepParametersFilePath() {
		return StepParametersFilePath;
	}


	/**
	 * @param stepParametersFilePath the stepParametersFilePath to set
	 */
	public void setStepParametersFilePath(String stepParametersFilePath) {
		StepParametersFilePath = stepParametersFilePath;
	}
	
	
	
}
